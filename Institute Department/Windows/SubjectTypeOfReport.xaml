<Window x:Class="Institute_Department.Windows.SubjectTypeOfReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Institute_Department.Windows"
        mc:Ignorable="d"
        Title="" Height="125" Width="315"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel Margin="0,0,0,0" HorizontalAlignment="Left" Width="300">
            <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                <Label>Вид зачета</Label>
                <ComboBox x:Name="TypeOfReportComboBox" DisplayMemberPath="Name" Width="200" Margin="19,0,0,0">
                </ComboBox>
            </StackPanel>
            <Button x:Name="SaveButton" Style="{StaticResource Button}" Content="Сохранить" Margin="114,10,96,0" Width="Auto" Click="SaveButton_Click"  />
        </StackPanel>
    </Grid>
</Window>
