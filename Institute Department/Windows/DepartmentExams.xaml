<Window x:Class="Institute_Department.Windows.DepartmentExams"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Institute_Department.Windows"
        mc:Ignorable="d"
        Height="155" Width="270"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0, 10, 0, 0">
                <Label>Зачет</Label>
                <TextBox x:Name="TestTextBox" IsReadOnly="True" Margin="82 0, -206, 0"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0, 10, 0, 0">
                <Label>Экзамен</Label>
                <TextBox x:Name="ExamTextBox" IsReadOnly="True" Margin="66, 0, -190, 0"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0, 10, 0, 0">
                <Label>Курсовая работа</Label>
                <TextBox x:Name="CourseworkTextBox" IsReadOnly="True" Margin="20, 0, -145, 0"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
